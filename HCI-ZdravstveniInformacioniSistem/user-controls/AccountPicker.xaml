<Window x:Class="HCI_ZdravstveniInformacioniSistem.user_controls.AccountPicker"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HCI_ZdravstveniInformacioniSistem.user_controls"
        mc:Ignorable="d"
        Height="150" Width="400" Background="#FF8CBDB7">
    <Grid>
        <Button x:Name="Doc" Style="{StaticResource CustomButton}" HorizontalAlignment="Left" Height="40" Margin="50,0,0,0" VerticalAlignment="Center" Width="91" Click="Doc_Click"/>
            <Button x:Name="Admin" Content="Admin" Style="{StaticResource CustomButton}" HorizontalAlignment="Right" Height="40" Margin="0,0,50,0" VerticalAlignment="Center" Width="91" Click="Admin_Click"/>

    </Grid>
</Window>
